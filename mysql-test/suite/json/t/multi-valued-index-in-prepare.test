CREATE TABLE t1 (
  f1 VARCHAR(50) NOT NULL PRIMARY KEY,
  f2 JSON NOT NULL,
  INDEX idx2 ( (CAST(f2 AS CHAR(50) ARRAY)) )
);

CREATE VIEW v1 AS
  SELECT * FROM t1, JSON_TABLE(f2, '$[*]' COLUMNS(i FOR ORDINALITY, id VARCHAR(50) PATH '$')) AS ids;

